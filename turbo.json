{
  "$schema": "https://turborepo.com/schema.json",
  "tasks": {
    "build": {
      "dependsOn": ["^build"]
    },
    "lint": {
      "dependsOn": ["^lint"]
    },
    "typecheck:lib": {
      "dependsOn": ["^typecheck:lib"]
    },
    "typecheck:test": {
      "dependsOn": ["^typecheck:lib"]
    },
    "test": {
      "dependsOn": []
    }
  }
}
